[
	{
		"if": [
			["компьютер_включается", false],
			["питание_подключено", true]
		],
		"then": ["проблема_с_блоком_питания", true]
	},
	{ "if": [["проблема_с_блоком_питания", true]], "then": ["требуется_замена_комплектующих", true] },
	{ "if": [["требуется_замена_комплектующих", true]], "then": ["стоимость_ремонта", "высокая"] },

	{
		"if": [
			["компьютер_включается", true],
			["операционная_система_загружается", false]
		],
		"then": ["проблема_с_жестким_диском", true]
	},
	{ "if": [["проблема_с_жестким_диском", true]], "then": ["возможна_потеря_данных", true] },
	{ "if": [["возможна_потеря_данных", true]], "then": ["требуется_бэкап", true] },

	{ "if": [["компьютер_перегревается", true]], "then": ["проблема_с_охлаждением", true] },
	{ "if": [["проблема_с_охлаждением", true]], "then": ["производительность", "низкая"] },

	{ "if": [["регулярная_чистка", true]], "then": ["компьютер_перегревается", false] },
	{ "if": [["регулярная_чистка", true]], "then": ["срок_службы", "увеличен"] },

	{
		"if": [
			["антивирус_установлен", true],
			["производительность", "низкая"]
		],
		"then": ["вредоносное_ПО", false]
	},
	{
		"if": [
			["антивирус_установлен", false],
			["производительность", "низкая"]
		],
		"then": ["вредоносное_ПО", true]
	},

	{ "if": [["оперативная_память_достаточна", false]], "then": ["многозадачность", "ограничена"] },
	{ "if": [["оперативная_память_достаточна", false]], "then": ["производительность", "низкая"] },

	{ "if": [["SSD_установлен", true]], "then": ["скорость_загрузки", "высокая"] },
	{ "if": [["SSD_установлен", true]], "then": ["производительность", "высокая"] },

	{ "if": [["дефрагментация_выполнена", true]], "then": ["скорость_доступа_к_файлам", true] },
	{ "if": [["скорость_доступа_к_файлам", true]], "then": ["производительность", "повышена"] },

	{
		"if": [
			["драйверы_обновлены", true],
			["производительность", "высокая"]
		],
		"then": ["совместимость_устройств", "максимальная"]
	},
	{
		"if": [
			["производительность", "низкая"],
			["вредоносное_ПО", true]
		],
		"then": ["требуется_переустановка_системы", true]
	}
]
